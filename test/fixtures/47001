 <id>0801.4072</id><submitter>Fayang Qiu</submitter><version version='v1'><date>Sat, 26 Jan 2008 12:14:06 GMT</date><size>117kb</size></version><version version='v2'><date>Fri, 14 Mar 2008 12:55:39 GMT</date><size>118kb</size></version><version version='v3'><date>Sun, 6 Apr 2008 13:54:54 GMT</date><size>114kb</size></version><title>The Riemann Hypothesis and the Nontrivial Zeros of the General
